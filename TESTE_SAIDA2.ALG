Algoritmo "teste_saida2"

Var

   a : inteiro
   b : real
   c : caractere
   d : logico

Inicio

   a <- 32
   b <- 2.34567
   c <- "Bom dia!"
   d <- VERDADEIRO
   
   escreval(a)
   escreval(b)
   escreval(b:10)
   escreval(b:10:2)
   escreval(c)
   escreval(d)

Fimalgoritmo